        android:id="@+id/et1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:singleLine="true"
        android:maxLength="10"
        android:imeOptions="actionSearch"/>

    <TextView
        android:id="@+id/tv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="6dp"
        android:textSize="12sp"
        android:text="123456789abcdeijklmnf....."/>

    <TextView
        android:id="@+id/tv2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

    <com.example.administrator.sometest.MeasureLayoutTest.ProfileNameLayout2
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginTop="16.5dp">

        <TextView
            android:background="#918"
            android:id="@+id/tv_name2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:ellipsize="end"
            android:lines="1"
            android:textColor="#FF2A2A30"
            android:textSize="20sp"
            android:text="AJohnDoeabcedeffseefs12345678910245" />

        <ImageView
            android:id="@+id/tv_name_edit2"
            android:minWidth="34dp"
            android:minHeight="34dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/tv_name2"
            android:background="@drawable/ic_match_female"/>

    </com.example.administrator.sometest.MeasureLayoutTest.ProfileNameLayout2>

    <com.example.administrator.sometest.MeasureLayoutTest.ProfileNameLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginTop="16.5dp">

        <TextView
            android:background="#987"
            android:id="@+id/tv_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:ellipsize="end"
            android:lines="1"
            android:textColor="#FF2A2A30"
            android:textSize="20sp"
            android:text="JohnDoeabcedef123456789" />

        <ImageView
            android:id="@+id/tv_name_edit"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_toRightOf="@+id/tv_name"
            android:background="@drawable/ic_match_female"
            android:layout_toEndOf="@+id/tv_name"
            android:layout_marginEnd="10dp"
            android:layout_marginStart="10dp" />

    </com.example.administrator.sometest.MeasureLayoutTest.ProfileNameLayout>


