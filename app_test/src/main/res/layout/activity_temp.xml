<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical">

    <FrameLayout
        android:id="@+id/fl_1"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:background="#f00"
        android:foreground="#88000000">


    <com.example.administrator.sometest.TmpActivity.TmpView
        android:id="@+id/tv1"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:background="#876"/>


        <TextView
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:background="#fff"/>

    </FrameLayout>

    <com.example.administrator.sometest.TmpActivity.ForceGroundLayout
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:background="#f00">

    </com.example.administrator.sometest.TmpActivity.ForceGroundLayout>

    <com.example.administrator.sometest.TmpActivity.TouchTestLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/tv_first"
            android:layout_width="match_parent"
            android:layout_height="50dp"/>

    </com.example.administrator.sometest.TmpActivity.TouchTestLayout>



